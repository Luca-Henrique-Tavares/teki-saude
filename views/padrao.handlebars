    <div class="mini_linha"></div>
    <p class="mat_img_title">ReferÃªncias:</p>
    {{#each referencias}}
    <div class="mat_cnt3">
        <p class="mat_ref_txt">{{this.ref}}</p>
    </div>
    {{/each}}
</div>
